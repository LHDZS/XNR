/**index.wxss**/
@import "../weui.wxss"; 

.body {
  width: 100%;
  height: auto;
}
/* 标头 */
.ty_hd_tit {
  width: 100%;
  height: 240rpx;
  line-height: 100rpx;
}
/* 标头名字 */
.name {
  font-size: 50rpx;
  margin: 50rpx 30rpx 20rpx;
}
/* 标头介绍 */
.ty_tit_msg {
  font-size: 35rpx;
  margin: 20rpx 30rpx 30rpx;
}
/* logo图标 */
.ty_logo {
  position:absolute;
  right:40rpx;
  top:95rpx;
  border-radius:50%;
  width:120rpx;
  height:120rpx;
  border:1rpx solid #000;
  background-image:url(https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1513768718212&di=4f4a41eb20d5b00103a048c59888f21d&imgtype=0&src=http%3A%2F%2Fimg15.3lian.com%2F2015%2Ff2%2F49%2Fd%2F115.jpg);
  background-size:contain;

}
/* 视频 */
.shipin {
  width: 100%;
  margin: 0;
  padding: 0;
}
/* 消毒器图片 */
 .xdqimg {
  width: 100%;
} 
.xdqimg1 {
  width: 100%;
  height: 1100rpx;
}
/* 血压计图片 */
.xue {
  width: 100%;
  height:360px;

}
/* 门店详情 */
.mt {
  margin-top: 20rpx;
  text-align: center;
  font-size: 40rpx;
  margin-bottom: 20rpx;
}
/* 门店视频 */
/* .m1 {
  margin-top:10rpx; 
} */
/* 睡佳图片 */
.timg {
  width: 100%;
} 
.timg1 {
  float: left;
  width: 50%;
}
/* TAP栏切换 */
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__content{
    /* padding-top: 60px; */
    margin: 0;
    padding: 0;
    text-align: center;
}
.weui-tab__content {
  padding:0;
}
/* 位置和电话 */
.weui-cell {
  padding:50rpx 30rpx;
}
.weui-cell__bd {
  margin-left: 10rpx;
  font-size: 37rpx;
}
/* 小泥人下标 */
.xiaoniren{
  width: 100%;
  height: auto;
  background-color: #fff;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  color: #555;
  }

/* 字体 */
  @font-face {
  font-family: "iconfont";
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 37rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

